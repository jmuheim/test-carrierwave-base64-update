<p>TRY TO CHANGE THE UPLOADS!</p>

<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <fieldset>
    <legend>User</legend>
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :file_upload %>
      <%= f.input :string_upload, as: :text %>
    </div>
  </fieldset>

  <fieldset>
    <legend>Attachments</legend>
    <div id="attachments">
      <%= f.simple_fields_for :attachments do |attachment| %>
        <%= render 'attachment_fields', f: attachment %>
      <% end %>

    <div class="links">
      <%= link_to_add_association 'add attachment', f, :attachments %>
    </div>
  </fieldset>

  <fieldset>
    <legend>Base64 image 1</legend>
    <textarea rows="6">data:image/png;base64,</textarea>
    <%= image_tag 'data:image/png;base64,', height: 100 %>
  </fieldset>

  <fieldset>
    <legend>Base64 image 2</legend>
    <textarea rows="6">data:image/png;base64,</textarea>
    <%= image_tag 'data:image/png;base64,', height: 100 %>
  </fieldset>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
